{
  "version": 2,
  "functions": {
    "server.js": { "runtime": "@vercel/node@18.x" }
  },
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/server.js" },
    { "source": "/admin-dashboard", "destination": "/server.js" },
    { "source": "/user-portal", "destination": "/server.js" },
    { "source": "/contract", "destination": "/server.js" },
    { "source": "/admin", "destination": "/server.js" },
    { "source": "/", "destination": "/server.js" }
  ]
}
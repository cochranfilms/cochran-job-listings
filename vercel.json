{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/(.*)\\.json$",
      "dest": "/$1.json"
    },
    {
      "src": "/(.*)\\.js$",
      "dest": "/$1.js"
    },
    {
      "src": "/(.*)\\.css$",
      "dest": "/$1.css"
    },
    {
      "src": "/(.*)\\.png$",
      "dest": "/$1.png"
    },
    {
      "src": "/(.*)\\.jpg$",
      "dest": "/$1.jpg"
    },
    {
      "src": "/(.*)\\.jpeg$",
      "dest": "/$1.jpeg"
    },
    {
      "src": "/(.*)\\.gif$",
      "dest": "/$1.gif"
    },
    {
      "src": "/(.*)\\.ico$",
      "dest": "/$1.ico"
    },
    {
      "src": "/(.*)\\.svg$",
      "dest": "/$1.svg"
    },
    {
      "src": "/(.*)\\.pdf$",
      "dest": "/$1.pdf"
    },
    {
      "src": "/(.*)\\.ttf$",
      "dest": "/$1.ttf"
    },
    {
      "src": "/(.*)\\.woff$",
      "dest": "/$1.woff"
    },
    {
      "src": "/(.*)\\.woff2$",
      "dest": "/$1.woff2"
    },
    {
      "src": "/admin-dashboard",
      "dest": "/server.js"
    },
    {
      "src": "/user-portal", 
      "dest": "/server.js"
    },
    {
      "src": "/contract",
      "dest": "/server.js"
    },
    {
      "src": "/admin",
      "dest": "/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}